{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build release",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/authService.csproj",
                "--configuration",
                "Release"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build debug",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/authService.csproj",
                "--configuration",
                "Debug"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}
